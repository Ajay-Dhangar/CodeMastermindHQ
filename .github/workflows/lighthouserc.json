{
    "ci": {
      "collect": {
        "startServerCommand": "npm run start:website",
        "startServerReadyPattern": "Serving",
        "startServerReadyTimeout": 10000,
  
        "numberOfRuns": 1,
  
        "settings": {
          "skipAudits": [
            "robots-txt",
            "canonical",
            "tap-targets",
            "is-crawlable",
            "works-offline",
            "offline-start-url"
          ]
        }
      }
    }
  }
  